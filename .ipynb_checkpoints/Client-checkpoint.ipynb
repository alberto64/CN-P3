    "import import_ipynb\n",
    "import SAWClient\n",
    "\n",
    "exit = False\n",
    "client = SAWClient.SAWClient('127.0.0.1')\n",
    "\n",
    "while not exit:\n",
    "    message = input(\"Send message:\")\n",
    "    success = client.sendMessage(message)\n",
    "    if message == \"exit\" or not success:\n",
    "        exit = True\n",
    "\n",
    "client.closeConnection()"
   ]
  }
